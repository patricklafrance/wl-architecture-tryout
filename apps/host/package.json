{
    "name": "@apps/host",
    "description": "ShareGate Home application",
    "private": true,
    "author": "Workleap",
    "version": "0.0.0",
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "postinstall": "pnpm msw init",
        "dev": "rsbuild dev --config rsbuild.dev.ts",
        "build": "pnpm run --sequential \"/^build:.*/\"",
        "build:app": "rsbuild build --config rsbuild.build.ts",
        "build:edge-functions": "rslib build --config rslib.edge-functions.ts",
        "serve-build": "pnpm http-server dist/app -p 8081 -P http://localhost:8081? -c-1 --cors",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "stylelint": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-location node_modules/.cache/stylelint --max-warnings=0",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@hopper-ui/components": "1.15.8",
        "@hopper-ui/icons": "2.10.4",
        "@hopper-ui/styled-system": "2.5.14",
        "@modules/management": "workspace:*",
        "@modules/migration": "workspace:*",
        "@opentelemetry/api": "1.9.0",
        "@packages/shell": "workspace:*",
        "@squide/firefly": "15.0.5",
        "@tanstack/query-core": "5.90.5",
        "@tanstack/react-query": "5.90.5",
        "@workleap/logging": "1.3.2",
        "@workleap/telemetry": "2.1.6",
        "logrocket": "10.1.0",
        "react": "19.2.0",
        "react-aria": "3.44.0",
        "react-aria-components": "1.13.0",
        "react-dom": "19.2.0",
        "react-router": "7.9.5"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@rsbuild/core": "1.6.0",
        "@rspack/core": "1.6.1",
        "@types/react": "19.2.2",
        "@types/react-dom": "19.2.2",
        "@typescript-eslint/parser": "8.46.2",
        "@workleap/browserslist-config": "2.1.3",
        "@workleap/eslint-configs": "1.1.5",
        "@workleap/netlify-skew-protection": "1.0.4",
        "@workleap/rsbuild-configs": "3.1.4",
        "@workleap/stylelint-configs": "2.1.3",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.38.0",
        "http-server": "14.1.1",
        "msw": "2.11.6",
        "prettier": "3.6.2",
        "storybook": "9.1.16",
        "storybook-react-rsbuild": "2.1.3",
        "stylelint": "16.25.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2"
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
