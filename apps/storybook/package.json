{
    "name": "@apps/storybook",
    "description": "Storybook application.",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "postinstall": "pnpm msw init",
        "dev": "storybook dev -p 6006",
        "build": "storybook build --disable-telemetry",
        "serve-build": "pnpm http-server storybook-static -p 8080 -P http://localhost:8080? -c-1",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "doctor": "storybook doctor"
    },
    "dependencies": {
        "@hopper-ui/components": "1.15.8",
        "@hopper-ui/icons": "2.10.4",
        "@hopper-ui/styled-system": "2.5.14",
        "@squide/firefly": "15.0.5",
        "@tanstack/react-query": "5.90.5",
        "msw": "2.11.6",
        "react": "19.2.0",
        "react-aria": "3.44.0",
        "react-aria-components": "1.13.0",
        "react-dom": "19.2.0",
        "react-router": "7.9.4"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@rsbuild/core": "1.6.0",
        "@storybook/addon-a11y": "10.0.5",
        "@types/react": "19.2.2",
        "@types/react-dom": "19.2.2",
        "@typescript-eslint/parser": "8.46.2",
        "@workleap/eslint-configs": "1.1.5",
        "@workleap/rsbuild-configs": "3.1.4",
        "@workleap/typescript-configs": "3.0.7",
        "chromatic": "13.3.3",
        "eslint": "9.38.0",
        "http-server": "14.1.1",
        "msw": "2.11.6",
        "msw-storybook-addon": "2.0.6",
        "react-aria": "3.44.0",
        "react-aria-components": "1.13.0",
        "storybook": "10.0.5",
        "storybook-react-rsbuild": "2.1.4",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2"
    },
    "msw": {
        "workerDirectory": [
            ".storybook/public"
        ]
    }
}
